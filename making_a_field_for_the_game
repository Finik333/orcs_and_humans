#include <iostream>
using namespace std;
int main(){
    setlocale(0, "rus");
    cout << "ОРКИ И ЛЮДИ" << endl;
    int i;
    int i1;
    int i2;
    int i3;
    int i4;
    for (i = 0; i < 100; i++) {
        cout << "#";      
    } for (i4 = 0; i4 < 25; i4++) {
        cout << "#" << endl;
        for (i1 = 0; i1 < 1; i1++) {
            cout << "#";
        }
        for (i3 = 0; i3 < 99; i3++) {
            cout << " ";      
        }
    }               
    return 0;
}
